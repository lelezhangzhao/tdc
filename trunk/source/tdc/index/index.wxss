page {
  background-color: #F8F8F8;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
}
.page-swiper{
  position:relative;
  
  padding-top:60rpx;
}
.page-section-spacing{
  box-sizing: border-box;
  padding: 0 80rpx;
}

.page-search{
  background-color: #FFFFFF;
  position:relative;
  margin-top:-20rpx;
  display:block;
  width:400rpx;
  height:50rpx;
  left:50%;
  margin-left:-200rpx;
  padding:0 20rpx;
  z-index: 1;
}


.border-radius-circle-30{
  border: 1rpx solid #CCCCCC;
  border-radius: 30rpx;
}

.search-place-holder{
  display:inline;
  float:right;
  font-size:30rpx;
  color: #AAA;
}

.page-find-t-s{
  position:relative;
  margin-top:20rpx;
  padding:0 20rpx;
}