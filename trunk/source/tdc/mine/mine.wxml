<!--tdc/mineteacher/mineteacher.wxml-->
<view cass="page">
  <view>
    {{nickName}}
    <view bindtap="preview">
      <image src="https://localtdc.com/static/image/my.png" />
      {{logo}}
    </view>
  </view>

  <view>
    <button bindtap="publishRecord">发布</button>
    <button bindtap="collectionRecord">收藏</button>
    <button bindtap="footerRecord">足迹</button>
  </view>

  <view>
    <view wx:if="{{isPublishList}}">
      <view wx:for="{{publishList}}" wx:key="">
        <view bindtap="publishInfo" data-id="{{item.id}}">
          {{item.id}}
        </view>
      </view>
    </view>

    <view wx:if="{{isCollectionList}}">
      <view wx:for="{{collectionList}}" wx:key="">
        <view bindtap="collectionInfo" data-id="{{item.id}}" data-object="{{item.publishobject}}">
          {{item.id}}
        </view>
      </view>
    </view>

    <view wx:if="{{isFooterList}}">
      <view wx:for="{{footerList}}" wx:key="">
        <view bindtap="footerInfo" data-id="{{item.id}}" data-object="{{item.publishobject}}">
          {{item.id}}
        </view>
      </view>
    </view>
  </view>

</view>
